.menu-dropdown li.remain .level1,
.menu-dropdown li.active .level1 {
    color: #a12f00; }

/* Drop-Down */
.menu-dropdown .dropdown {
    top: 42px;
    border-top: 1px solid #DDD; }

.menu-dropdown .dropdown-bg > div {
    padding: 0 5px;
    border: 1px solid #DDD;
