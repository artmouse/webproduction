$j(function () {
    $j('.js-date-checked').click(function () {
        var date = $j(this).data('date');
        $j('.js-input-check[data-date=' + date + ']').click();

    });
    
    $j('.js-time-checked').click(function () {
        var time = $j(this).data('time');
        $j('.js-input-check[data-time="' + time + '"]').click();
    });
});
