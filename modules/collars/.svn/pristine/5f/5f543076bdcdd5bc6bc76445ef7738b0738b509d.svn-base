$sub-large-breakpoint: 1281px;
$large-breakpoint: 1601px;

[class*="block-grid-"] {
    display: block;
    padding: 0;
    margin: 0;
}

[class*="block-grid-"]:before, [class*="block-grid-"]:after {
    content: " ";
    display: table
}

[class*="block-grid-"]:after {
    clear: both
}

[class*="block-grid-"] > li {
    display: block;
    height: auto;
    float: left;
    width: 100%;
}

