}

.cl-seo {
    box-shadow: 1px 1px 5px rgb(204, 204, 204);
    margin: 0 0 32px;
    padding: 22px 28px;
    font-size: $baseFontSize + 1;
    line-height: 20px;

    &.index-seo {
        position: absolute;
        left: -9999px;
    }
}

.cl-product-list {
    margin: 0 0 20px 0;
}

.cl-product-thumb {
    width: 200px;
    margin: 0 auto;
    position: relative;
    overflow: hidden;

    .icon-image {
        position: absolute;
        z-index: 10;
        left: auto;
        right: 5px;
        top: 3px;
        max-width: 50px !important;
    }

    &:hover {
        .hidden-descript {
            top: 0;
            left: 0;
            z-index: 5;
        }

        .icon-image {
            display: none;
        }
    }

    .block-price {
        color: $colorBright;
        text-align: center;
        font-size: 18px;
        font-weight: 600;
        padding: 0 10px;
        margin: 0 0 8px 0;

        div {
            color: $colorBright;
            text-align: center;
            font-size: 18px;
            font-weight: 600;

            &.os-price-unavailable {
                color: #ccc;
            }

            &.os-price-old {
                color: #333;
                text-decoration: line-through;
            }

            &.new-price {
                color: #E6009C;
            }

        }
    }

    .name {
        padding: 6px 10px 0 10px;
        margin: 0 0 6px 0;
        text-align: center;
        max-height: 92px;
        overflow: hidden;
        text-overflow: ellipsis;
        line-height: 17px;

        a {
            text-decoration: none;
            font-size: 18px;

            &:hover {
                text-decoration: underline;
            }
        }
    }

    .block-button {
        text-align: center;

        div {
            display: inline-block;
            vertical-align: middle;
        }
    }

    .hidden-descript {
        position: absolute;
        width: 100%;
        height: 100%;
        left: -100%;
        top: 100%;
        background-color: rgba(250, 250, 250, 0.4);
        @include transition-duration(0.25s);
    }

    .block-image {
        width: 200px;
        height: 200px;

        a {
            width: 200px;
            height: 200px;
            display: table-cell;
