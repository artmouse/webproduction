$baseFont: Helvetica, Arial, sans-serif;
$baseFontSize: 14px;
$colorDark: #333333;
$colorBase: #4B3326;
$colorSuccess: #5CB85C;
$colorBright: #337AB7;
$colorImportant: #EA3A3C;
$iconBg:  url("../_images/icons.png") no-repeat;

@mixin box-sizing($boxsizing) {
    -moz-box-sizing: $boxsizing;
    -webkit-box-sizing: $boxsizing;
    box-sizing: $boxsizing;
}
$bb: border-box;

@mixin product-scale ($scale) {
    -moz-transform: scale($scale, $scale);
    -webkit-transform: scale($scale, $scale);
    -o-transform: scale($scale, $scale);
    -ms-transform: scale($scale, $scale);
    transform: scale($scale, $scale);
}

@mixin transition-duration($duration) {
    -webkit-transition-duration: $duration;
    -moz-transition-duration: $duration;
    -o-transition-duration: $duration;
    -ms-transition-duration: $duration;
    transition-duration: $duration;
}

html, body {
    height: 100%;
    width: 100%;
}

body {
    background: #fff;
    text-decoration: none;
    display: table;
}

body, form, div, p, a, img, h1, h2, h3, h4, h5, h6, table, table tr, table td {
    border: none;
    color: $colorDark;
    font-family: $baseFont;
    font-size: $baseFontSize;
    font-style: normal;
    font-weight: normal;
    margin: 0;
    padding: 0;
}

input,
textarea,
select {
    outline: none;
    font-family: $baseFont;
    font-size: $baseFontSize + 1;
    @include box-sizing($bb);
}

input::-moz-focus-inner {
    border: 0;
    padding: 0;
}

input[type="text"],
input[type="password"],
input[type="tel"],
input[type="email"] {
    border: 1px solid #CCCCCC;
    display: inline-block;
    vertical-align: middle;
    padding: 0 16px;
    height: 34px;
    box-shadow: none;
    border-radius: 3px;
    -webkit-appearance: none;
    @include box-sizing($bb);
    width: 100%;
    background-color: #fff;
    font-size: $baseFontSize;
    color: $colorDark;

    &:focus {
        border: 1px solid #66AFE9;
        -webkit-box-shadow: 0 0 5px 0 rgba(102, 175, 233, 0.6);
        -moz-box-shadow: 0 0 5px 0 rgba(102, 175, 233, 0.6);
        box-shadow: 0 0 5px 0 rgba(102, 175, 233, 0.6);
    }
}

textarea {
    box-shadow: none;
    border-radius: 3px;
    @include box-sizing($bb);
    width: 100%;
    background-color: #fff;
    -webkit-appearance: none;
    border: 1px solid #CCCCCC;
    min-height: 100px;
    resize: vertical;
    font-size: $baseFontSize;
    color: $colorDark;
    padding: 8px 16px;
}

select {
    background-color: #ffffff;
    border: 1px solid #ccc;
    border-radius: 3px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    color: $colorDark;
    display: block;
    font-size: 14px;
    height: 34px;
    transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
    padding: 0 10px;

    &:focus {
        border: 1px solid #66AFE9;
        -webkit-box-shadow: 0 0 5px 0 rgba(102, 175, 233, 0.6);
        -moz-box-shadow: 0 0 5px 0 rgba(102, 175, 233, 0.6);
        box-shadow: 0 0 5px 0 rgba(102, 175, 233, 0.6);
    }
}

.cl-select {
    border: 1px solid #ccc;
    border-radius: 3px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    color: $colorDark;
    display: inline-block;
    vertical-align: middle;
    position: relative;
    font-size: 14px;
    height: 34px;
    line-height: 34px;
    transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
    padding: 0 10px;
    background: url("/modules/collars/_images/select-arr.png") right center no-repeat #ffffff;

    select {
        position: absolute;
        left: 0;
        top: 0;
        height: 34px;
        line-height: 34px;
        padding: 0 !important;
        margin: 0 !important;
        width: 100%;
        opacity: 0;
    }

    &.focus {
        border: 1px solid #66AFE9;
        -webkit-box-shadow: 0 0 5px 0 rgba(102, 175, 233, 0.6);
        -moz-box-shadow: 0 0 5px 0 rgba(102, 175, 233, 0.6);
        box-shadow: 0 0 5px 0 rgba(102, 175, 233, 0.6);
    }
}

strong {
    font-weight: 700;
}

input::-moz-focus-inner {
    border: 0;
    padding: 0;
}

a {
    text-decoration: underline;
    outline: none;
}

a:hover {
    text-decoration: none;
}

h1, h2 {
    font-weight: 500;
    font-size: 30px;
    color: #333333;
    margin: 0 0 10px 0;
    line-height: normal;
}

h3 {
    font-size: 24px;
    margin: 0 0 10px;
    text-transform: none;
}

.clear {
    clear: both;
    font-size: 0;
    line-height: 0;
    height: 0;
}

.fl-l {
    float: left;
}

.fl-r {
    float: right;
}

.important {
    color: rgba(255, 0, 0, 0.70);
}

.ta-center {
    text-align: center !important;
}

.ta-left {
    text-align: left !important;
}

.ta-right {
    text-align: right !important;
}

.va-top {
    vertical-align: top !important;
}

.nowrap {
    white-space: nowrap !important;
}

.cl-content-wrapper {
    display: table-row;

    .wrapper-cell {
        display: table-cell;

        &:not(*:root) {
            height: 100%;
        }
    }
}

.cl-header-wrapper {
    display: table-header-group;

    .wrapper-cell {
        display: table-cell;
        height: 1px;
    }
}

.cl-footer-wrapper {
    display: table-footer-group;

    .wrapper-cell {
        display: table-cell;
        height: 1px;
    }
}

.cl-mainer {
    width: 1580px;
    padding: 0 10px;
    margin: 0 auto;
}

.cl-button {
    display: inline-block;
    vertical-align: middle;
    @include box-sizing($bb);
    padding: 0 20px;
    height: 42px;
    text-align: center;
    text-decoration: none;
    font-size: 14px;
    border-radius: 6px;
    border: 1px solid #2E6DA4;
    background-color: #337AB7;
    cursor: pointer;
    color: #fff;
    position: relative;

    &.grey {
        color: #333333;
        background-color: #fff;
        border: 1px solid #CCCCCC;

        &:hover {
            background-color: #E6E6E6;
            color: #333333;
        }
    }

    &.small {
        height: 35px;
        border-radius: 3px;
    }

    &.login {

        span {
            display: inline-block;
            vertical-align: top;
            width: 15px;
            height: 15px;
            background: $iconBg;
            background-position: 0 -655px;
        }
    }

    &.remove {
        background-color: #D9534F;
        border: 1px solid #D43F3A;
        padding: 0 12px 0 32px;

        &:after {
            content: '';
            position: absolute;
            width: 12px;
            height: 13px;
            left: 14px;
            top: 10px;
            background: $iconBg;
            background-position: 0 -682px;
        }

        &:hover {
            background-color: darken(#D9534F, 12%);
        }
    }

    &.green {
        background-color: $colorSuccess;
        border: 1px solid #4CAE4C;

        &:hover {
            background-color: darken($colorSuccess, 12%);
        }

        &:active {
            box-shadow: 0 3px 5px rgba(0, 0, 0, 0.125) inset;
        }
    }

    &.continue {
        height: 35px;
        line-height: 33px;
        padding: 0 10px 0 34px;
        border: 1px solid #CCCCCC;
        color: $colorBright;
        background-color: #fff;
        border-radius: 3px;

        &:after {
            content: '';
            position: absolute;
            width: 16px;
            height: 16px;
            top: 50%;
            margin: -8px 0 0 0;
            left: 10px;
            background: $iconBg;
            background-position: 0 -112px;
        }

        &:active {
            background-color: #E6E6E6;
            box-shadow: 0 3px 5px rgba(0, 0, 0, 0.125) inset;
            color: #333333;

            &:after {
                background-position: 0 -112px;
            }
        }

        &:hover {
            background-color: #E6E6E6;
        }
    }

    &.buy-button {
        height: 32px;
        line-height: 30px;
        font-size: $baseFontSize - 1;
        padding: 0 9px 0 30px;

        &:active {
            top: 0;
            left: 0;
        }

        &:after {
            content: '';
            position: absolute;
            width: 16px;
            height: 16px;
            background: $iconBg;
            background-position: 0 -136px;
            left: 10px;
            top: 50%;
            margin: -8px 0 0 0;
        }
    }

    &.basket {
        padding: 0 9px 0 30px;
        background-color: $colorSuccess;
        border: 1px solid #4CAE4C;

        &:hover {
            background-color: darken($colorSuccess, 12%);
        }

        &:active {
            box-shadow: 0 3px 5px rgba(0, 0, 0, 0.125) inset;
        }

        &:after {
            content: '';
            position: absolute;
            width: 16px;
            height: 16px;
            background: $iconBg;
            background-position: 0 -136px;
            left: 10px;
            top: 50%;
            margin: -9px 0 0 0;
        }
    }

    &:hover {
        background-color: darken($colorBright, 12%);
    }

    &:active {
        position: relative;
        top: 1px;
        left: 1px;
        box-shadow: 0 3px 5px rgba(0, 0, 0, 0.125) inset;
    }
}

a.cl-button {
    line-height: 42px;
}

a.cl-button.small {
    line-height: 32px;
}

.cl-fullwidth-slider {
    position: relative;
    height: auto;
    @include box-sizing($bb);
    margin: -20px auto 20px auto;
    overflow: hidden;

    img {
        display: none;
    }

    .description {
        position: absolute;
        left: 0;
        top: auto;
        bottom: 58px;
        width: 100%;
        text-align: center;
        color: #fff;
        text-shadow: 0 1px 2px rgba(0, 0, 0, 0.6);

        .title {
            font-size: $baseFontSize + 10;
            margin: 0 0 10px 0;
            color: #fff;
            display: inline-block;
        }
    }

    .slick-slide {
        float: left;
        min-height: 1px;
        display: none;
        text-align: center;
        position: relative;
        height: 100%;

        img {
            display: block;
            margin: 0 auto;
            max-width: 100%;
            max-height: 600px;
        }
    }

    &.slick-initialized {
        .slick-slide {
            display: block;
        }
    }

    .slick-dots {
        list-style: none;
        position: absolute;
        z-index: 12;
        margin: 0;
        padding: 0;
        width: 100%;
        text-align: center;
        top: auto;
        bottom: 36px;
        left: 0;

        li {
            display: inline-block;
            vertical-align: middle;
            margin: 0 2px;

            &.slick-active {
                button {
                    width: 12px;
                    height: 12px;
                    background-color: #ffffff;
                }
            }
        }

        button {
            border: 1px solid #fff;
            background-color: transparent;
            color: transparent;
            font-size: 0;
            width: 10px;
            height: 10px;
            @include box-sizing($bb);
            border-radius: 50%;
            display: block;
            cursor: pointer;
            padding: 0;
            box-shadow: 0 0 1px rgba(0,0,0,0.4);

            &:focus {
                outline: none;
            }
        }
    }

    .slick-list {
        position: relative;
        overflow: hidden;
        display: block;
        margin: 0;
        padding: 0;
    }

    .slick-prev, .slick-next {
        z-index: 10;
        top: 0;
        height: 100%;
        position: absolute;
        width: 15%;
        //background-image: linear-gradient(to right, rgba(0, 0, 0, 0.5) 0px, rgba(0, 0, 0, 0) 100%);
        background-repeat: repeat-x;
        padding: 0;
        margin: 0;
        font-size: 0;
        color: transparent;
        border: none;
        opacity: 0.6;
        cursor: pointer;
        background-color: transparent;

        &:focus {
            outline: none;
        }

        &:hover {
            opacity: 0.9;
        }
    }

    .slick-next {
        left: auto;
        right: 0;
        //background-image: linear-gradient(to left, rgba(0, 0, 0, 0.5) 0px, rgba(0, 0, 0, 0) 100%);

        &:after {
            content: '';
            position: absolute;
            width: 19px;
            height: 30px;
            background: $iconBg;
            background-position: 0 -40px;
            left: 50%;
            top: 50%;
            margin: -15px 0 0 -9px;
            z-index: 11;
        }
    }

    .slick-prev {
        left: 0;

        &:after {
            content: '';
            position: absolute;
            width: 19px;
            height: 30px;
            background: $iconBg;
            background-position: 0 0;
            left: 50%;
            top: 50%;
            margin: -15px 0 0 -9px;
            z-index: 11;
        }
    }
}

.cl-nav-button {
    display: none;
    width: 44px;
    height: 34px;
    border-radius: 4px;
    @include box-sizing($bb);
    padding: 9px 10px;
    border: 1px solid #333;
    margin: 0 0 8px 0;

    > span {
        width: 100%;
        display: block;
        height: 2px;
        background-color: #333;
        margin: 4px 0 0 0;

        &:first-child {
            margin: 0;
        }
    }

    &:hover {
        background-color: #333;

        > span {
            background-color: #fff;
        }
    }
}

.cl-header {
    margin: 0 0 20px 0;

    .cl-mainer {
        border-bottom: 1px solid #DDDDDD;

    }

    .clear.hidden {
        display: none;
    }

    .logo-image {
        float: left;
        width: 136px;
        height: 34px;
        margin: 8px 24px 0 0;

        .inner-wrap {
            display: table-cell;
            width: 136px;
            height: 34px;
            vertical-align: middle;
        }

        img {
            display: block;
            margin: 0 auto;
            max-width: 134px;
            max-height: 32px;
        }
    }

    .block-auth {
        float: right;
        margin: 18px 0 0 20px;

        > a {
            display: inline-block;
            font-size: $baseFontSize - 1;
            text-decoration: none;
            margin: 0 8px 0 0;
            color: #333;
        }

        .enter-link {
            position: relative;
            padding: 0 0 0 16px;

            &:after {
                content: '';
                position: absolute;
                width: 9px;
                height: 12px;
                background: $iconBg;
                background-position: 0 -820px;
                left: 0;
                top: 50%;
                margin-top: -6px;
            }
        }

        .registr-link {
            position: relative;
            padding: 0 0 0 16px;

            &:after {
                content: '';
                position: absolute;
                width: 11px;
                height: 12px;
                background: $iconBg;
                background-position: 0 -848px;
                left: 0;
                top: 50%;
                margin-top: -6px;
            }
        }
    }

    .logo {
        float: left;
        padding: 6px 0;
        margin: 8px 26px 0 0;

        a {
            position: relative;
            font-size: $baseFontSize + 4;
            color: #9D9D9D;
            text-decoration: none;
            padding: 0 0 0 36px;
            display: inline-block;

            &:hover {
                color: #fff;
            }

            &:after {
                content: '';
                width: 30px;
                height: 30px;
                position: absolute;
                left: 0;
                top: 50%;
                margin: -15px 0 0 0;
                background: $iconBg;
                background-position: 0 -79px;
            }
        }
    }

    .search-toggle-button {
        display: none;
        float: right;
        height: 34px;
        width: 34px;
        background: $iconBg;
        background-position: 9px -730px;
        margin: 0 16px;

        &:hover {
            opacity: 0.75;
        }
    }

    .search {
        width: 209px;
        float: right;
        position: relative;
        margin: 9px 0 0 26px;

        input[type='text'] {
            padding: 0 16px 0 52px;
        }

        input[type='submit'] {
            width: 39px;
            height: 32px;
            position: absolute;
            border-left: none;
            border-bottom: none;
            border-top: none;
            border-right: 1px solid #ccc;
            top: 1px;
            left: 1px;
            background: #EEEEEE $iconBg;
            background-position: 10px -157px;
            cursor: pointer;

            &:hover {
                opacity: 0.75;
            }
        }
    }

    .basket-wrap {
        float: right;
        margin: 18px 0 0 16px;
    }

    .basket {

        text-decoration: none;
        color: #9D9D9D;
        position: relative;
        padding: 0 0 0 18px;


        &:before {
            content: '';
            position: absolute;
            width: 16px;
            height: 16px;
            background: $iconBg;
            background-position: 0 -112px;
            left: 0;
            top: 50%;
            margin: -8px 0 0 0;
        }

        &:hover {
            color: #000;
        }
    }
}

.cl-category-menu {
