/* slick default styles */

.slick-slider {
    position: relative;
    @include box-sizing($bb);

    .slick-list {
        position: relative;
        overflow: hidden;
        margin: 0;
        padding: 0;
    }

    .slick-track {
        position: relative;
        left: 0;
        top: 0;
        display: block;
    }

    .slick-slide {
        float: left;
        height: 100%;
        min-height: 1px;
        display: none;

        &:focus {
            outline: none;
        }

        img {
            display: block;
            margin: 0 auto;
        }
    }

    &.slick-initialized {
        .slick-slide {
            display: block;
        }
    }
}

