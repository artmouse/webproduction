    margin: 0;
    list-style: none;
    display: table;
    float: left;

    > li {
        display: table-cell;
        padding: 3px 13px 0 13px;
        vertical-align: middle;
        height: 50px;
        position: relative;
        max-width: 118px;

        //.sales {
        //    background: #E6009C;
        //    color: #fff;
        //    padding: 8px 10px;
        //    position: absolute;
        //    left: -20px;
