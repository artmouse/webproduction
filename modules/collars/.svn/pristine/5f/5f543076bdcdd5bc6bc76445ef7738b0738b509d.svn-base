        white-space: nowrap;
        font-weight: bold;
    }

    .name {
        width: 80%;
    }
}

.cl-block-popup {
    position: fixed;
    padding: 0 100px;
    margin: 0 -100px;
    overflow: auto;
    z-index: 1000;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0);

    &:before,
    &:after {
        content: '';
        display: block;
        height: 100px;
        width: 100%;
    }

    .dark {
        position: fixed;
        z-index: 1001;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: rgba(0,0,0,0.25);
    }

    .head {
        position: relative;
        padding: 35px 60px 35px 35px;
        font-size: $baseFontSize + 8;
        text-align: center;
        color: #333;
        background-color: #fff;
        border-bottom: 1px solid #ccc;

        .login-title {
            display: inline-block;
            vertical-align: middle;
            position: relative;
            font-size: $baseFontSize + 16;
            padding: 0 0 0 40px;

            &:after {
                content: '';
                position: absolute;
                background: $iconBg;
                background-position: 0 -611px;
                width: 29px;
                height: 30px;
                top: 50%;
                margin: -16px 0 0 0;
                left: 0;
            }
        }
    }

    .close {
        width: 16px;
        height: 16px;
        text-decoration: none;
        position: absolute;
        background: $iconBg;
        background-position: 0 -583px;
        top: 50%;
        margin: -8px 0 0 0;
        right: 30px;

        &:hover {
            opacity: 0.75;
        }
    }

    .body {
        padding: 35px 50px;
    }

    .block-form {

        .form-element {
            margin: 0 0 14px 0;

            .descript {
                font-weight: 700;
                margin: 0 0 6px 0;
            }
        }

        .cl-button.login {
            width: 100%;
            text-align: center;
        }

        label {
            display: inline-block;
            padding: 0 0 0 26px;
            margin: 0 0 6px 0;
            position: relative;

            input[type='radio'],
            input[type='checkbox'] {
                margin: 0;
                position: absolute;
                left: 0;
                top: 2px;
            }
        }
    }

    .block-popup {
        margin: 0 auto;
        background-color: #ffffff;
        position: relative;
        z-index: 1002;
        box-shadow: 0 20px 40px rgba(0, 0, 0, 0.30);
        width: 580px;
    }

    .popup-basket {
        width: 890px;
    }

    .message-error,
    .message-success {
        margin: 0 20px 20px 20px;
        border-top: 1px solid #d7d7d7;
        border-bottom: 1px solid #d7d7d7;
        text-align: center;
        font-size: $baseFontSize + 2;
        padding: 25px 0;

        a {
            font-size: $baseFontSize + 2;
        }

        div {
            color: $colorImportant;
        }

        span {
            font-size: $baseFontSize;
        }
    }

    .message-error {
        color: $colorImportant;
    }

    .message-success {
        color: $colorSuccess;
    }

    .foot {
        padding: 20px;

        .registr,
        .remind {
            color: $colorBright;
        }
    }

    .banner-image {
        display: block;
        width: 100%;
    }
}

.block-width {
    width: 400px;
}

