    .slick-prev, .slick-next {
        position: absolute;
        border: none;
        outline: none;
        font-size: 0;
        color: transparent;
        width: 30px;
        height: 30px;
        border-radius: 50%;
        background: $colorBase;
        top: 50%;
        margin: -15px 0 0 0;
        cursor: pointer;
        z-index: 10;
        visibility: hidden;

        &.slick-disabled {
            opacity: 0;
        }

        &:hover {
            opacity: 0.75;
        }
    }

    .slick-next {
        left: auto;
        right: 0;

        &:after {
            content: '';
            position: absolute;
            width: 6px;
            height: 9px;
            background: $iconBg;
            background-position: 0 -521px;
            top: 50%;
            left: 50%;
            margin: -4px 0 0 -3px;
        }
    }

    .slick-prev {
        left: 0;

        &:after {
            content: '';
            position: absolute;
            width: 6px;
            height: 9px;
            background: $iconBg;
            background-position: 0 -537px;
